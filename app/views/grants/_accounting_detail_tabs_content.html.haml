- unless @organization.chart_of_accounts.blank?
  .tab-pane.fade#gla
    .tab-content
      = render :partial => 'gla_table', :locals => {:gla_budgets => @grant.grant_budgets}

.tab-pane.fade#expn
  .tab-content
    - if @grant.expenditures.empty?
      %p There are no CapEx associated with this grant.
    - else
      = render :partial => 'expenditures/index_table', :locals => {:expenditures => @grant.expenditures}
