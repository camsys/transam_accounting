.tab-pane.fade#summ
  .tab-content
    = render 'vendor_to_date'

.tab-pane.fade#expn
  .tab-content
    - if @vendor.expenditures.empty?
      %p There are no CapEx associated with this vendor.
    - else
      = render :partial => 'assets/asset_compact_datatable', :locals => {:assets => @vendor.expenditures}
